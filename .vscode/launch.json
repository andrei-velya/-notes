    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\notes\\manage.py",
            "cwd": "${workspaceFolder}\\notes",
        },
        {
            "name": "Python: Runserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}\\notes\\manage.py",
            "cwd": "${workspaceFolder}\\notes",
            "args": [
                "runserver",
            ],
            "django": true
        },
    ]
}